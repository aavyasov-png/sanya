    "axios": "^1.13.3",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.2.0",
    "csv-parse": "^6.1.0",
    "dompurify": "^3.3.1",
    "groq-sdk": "^0.37.0",
    "jsdom": "^27.4.0",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
