{
  "name": "vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "check": "npm run typecheck && npm run lint && npm run build",
    "crawl": "node scripts/crawl.js",
    "vercel:dev": "vercel dev",
    "cf:dev": "wrangler pages dev dist --compatibility-date=2025-01-27",
    "cf:deploy": "wrangler pages deploy dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.1",
